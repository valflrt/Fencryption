{
  "name": "fencryption",
  "version": "1.4.8",
  "main": "src/index.ts",
  "bin": {
    "fencryption": "build/src/index.js"
  },
  "scripts": {
    "build": "npm ci && rm -rf build && npx tsc",
    "build:bin": "npm run build && rm -rf binaries && npx pkg . && sh scripts/compress-binaries.sh"
  },
  "author": "valflrt",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.39",
    "pkg": "^5.7.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^9.3.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "stream": "^0.0.2",
    "throat": "^6.0.1"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64",
      "node16-win-x64"
    ],
    "outputPath": "binaries"
  },
  "description": "A simple cli program to encrypt a file/directory",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valflrt/encrypt-directory.git"
  },
  "bugs": {
    "url": "https://github.com/valflrt/encrypt-directory/issues"
  },
  "homepage": "https://github.com/valflrt/encrypt-directory#readme",
  "keywords": [
    "file-encryption"
  ],
  "directories": {
    "doc": "docs"
  }
}
