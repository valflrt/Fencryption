{
  "name": "fencryption",
  "version": "1.3.4",
  "main": "src/index.ts",
  "bin": {
    "fencryption": "build/src/index.js"
  },
  "scripts": {
    "build": "rm -rf build && npx tsc",
    "build:binaries": "npm run build && npm ci && rm -rf binaries && npx pkg . && sh scripts/compress-binaries.sh"
  },
  "author": "valflrt",
  "license": "MIT",
  "devDependencies": {
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.39",
    "@types/node-gzip": "^1.1.0",
    "pkg": "^5.7.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "commander": "^9.3.0",
    "fs": "^0.0.1-security",
    "minimist": "^1.2.6",
    "node-gzip": "^1.1.2",
    "path": "^0.12.7",
    "readline": "^1.3.0"
  },
  "pkg": {
    "targets": [
      "node16-linux-x64",
      "node16-win-x64"
    ],
    "outputPath": "binaries"
  },
  "description": "A simple cli program to encrypt a directory and its content, made with nodejs and typescript.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valflrt/encrypt-directory.git"
  },
  "bugs": {
    "url": "https://github.com/valflrt/encrypt-directory/issues"
  },
  "homepage": "https://github.com/valflrt/encrypt-directory#readme",
  "keywords": [
    "file-encryption"
  ]
}
